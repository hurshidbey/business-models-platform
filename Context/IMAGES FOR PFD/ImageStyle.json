{
  "version": "1.0",
  "visual_style": {
    "palette": {
      "background_hex": "#FFFFFF",
      "primary_hex": "#000000",
      "accent_hex": "#E3A900",
      "secondary_hex": "#D9D9D9"
    },
    "shading": {
      "type": "flat_vector",
      "contrast": "medium",
      "highlight_use": "accent_only"
    },
    "halftone": {
      "enabled": true,
      "pattern_type": "dot_matrix",
      "density": "medium",
      "placement": "shadows_and_side_faces"
    },
    "lines": {
      "weight_px": 3,
      "style": "solid",
      "corner_type": "rounded",
      "arrow_type": "filled_triangle"
    },
    "composition": {
      "layout": "isometric_grid",
      "balance": "symmetrical_center_focus",
      "object_depth": "minimal_3D"
    },
    "rendering": {
      "lighting": "flat_no_shadows",
      "finish": "matte",
      "texture": "clean_vector"
    }
  }
}
